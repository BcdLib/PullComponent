﻿<div class="pull-container"
      @ontouchstart="OnTouchStart"
      @ontouchmove="OnTouchMove"
      @ontouchend="OnTouchEnd"
      @ontouchend:preventDefault="true"
      @ontouchend:stopPropagation="true"
>
    <div class="pull-wrapper" style="@GetWrapperStyle()">
        <div class="pull-tip pull-down-tip">
            @GetTipHtml()
        </div>
        @ChildContent
    </div>
</div>

